{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": 0,
        "semi": [
            "off"
        ],
        "quotes": "off",
        "jsx-quotes": [
            "error",
            "prefer-single"
        ],
        "object-curly-newline": [
            {
                "ImportDeclaration": "never"
            }
        ]
    }
}
